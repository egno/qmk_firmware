                {"matrix": [0, 2], "x": 2, "y": 0},
                {"matrix": [0, 3], "x": 3, "y": 0.2},
                {"matrix": [0, 4], "x": 4, "y": 0.4},

                {"matrix": [1, 0], "x": 0, "y": 1.8},
                {"matrix": [1, 1], "x": 1, "y": 1.2},
                {"matrix": [1, 2], "x": 2, "y": 1},
                {"matrix": [1, 3], "x": 3, "y": 1.2},
                {"matrix": [1, 4], "x": 4, "y": 1.4},

                {"matrix": [2, 0], "x": 0, "y": 2.8},
                {"matrix": [2, 1], "x": 1, "y": 2.2},
                {"matrix": [2, 2], "x": 2, "y": 2},
                {"matrix": [2, 3], "x": 3, "y": 2.2},
