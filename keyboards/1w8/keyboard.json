{
    "usb": {
        "device_version": "0.0.1",
        "no_startup_check": true
    },
    "features": {
        "bootmagic": true,
        "extrakey": true,
        "mousekey": true,
        "nkro": false,
        "unicode": true
    },
    "processor": "atmega32u4",
